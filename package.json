{
    "name": "@grouptogawa/sakiko-adapter-onebot",
    "version": "0.3.0",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/grouptogawa/sakiko-adapter-onebot.git"
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts --out-dir dist --sourcemap --clean",
        "prepublishOnly": "npm run build"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "onebot",
        "v11",
        "chatbot",
        "adapter"
    ],
    "license": "MIT",
    "author": "machinacanis",
    "contributors": [
        "MCredbear"
    ],
    "devDependencies": {
        "@types/node": "^24.10.3",
        "@types/ws": "^8.18.1",
        "prettier": "^3.7.4",
        "tsup": "^8.5.1"
    },
    "peerDependencies": {
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "ws": "^8.18.3"
    },
    "optionalDependencies": {
        "@grouptogawa/sakiko": "^0.4.8",
        "chalk": "^5.6.2"
    }
}
